{
    "manifest_version": 2,
    "name": "Worklog",
    "version": "0.1.0",
    "description": "Chrome extension to log hourses",
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["index.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}
